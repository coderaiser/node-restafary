{
  "name": "restafary",
  "version": "3.0.3",
  "description": "REST for CRUD file operations",
  "main": "server/restafary.js",
  "engines": {
    "node": ">=4"
  },
  "babel": {
    "presets": [
      "env"
    ]
  },
  "scripts": {
    "test": "tape 'test/*.js'",
    "fix:jscs": "jscs --esnext client server test --fix",
    "lint:jshint": "jshint server test",
    "lint:jscs": "jscs --esnext server test",
    "report": "nyc report --reporter=text-lcov | coveralls",
    "coverage": "nyc npm test",
    "watch:test": "npm run watcher -- npm test",
    "watch:lint": "npm run watcher -- npm run lint",
    "watcher": "nodemon -w test -w server --exec",
    "compile": "webpack --progress",
    "compile:dev": "NODE_ENV=development npm run compile",
    "build": "redrun clean compile*",
    "wisdom": "npm run build",
    "lint": "redrun lint:*",
    "lint:client": "eslint --env browser client",
    "lint:server": "eslint server test",
    "clean": "rimraf dist*"
  },
  "dependencies": {
    "ashify": "^1.0.0",
    "beautifile": "^1.0.0",
    "checkup": "^1.3.0",
    "es6-promisify": "^6.0.0",
    "files-io": "^1.2.1",
    "flop": "^3.0.0",
    "jonny": "^1.0.1",
    "mellow": "^2.0.0",
    "minify": "^3.0.2",
    "patchfile": "^1.0.0",
    "ponse": "^1.4.1",
    "pullout": "^1.0.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "coveralls": "^3.0.0",
    "eslint": "^4.0.0",
    "express": "^4.13.4",
    "jscs": "^3.0.3",
    "jshint": "^2.9.1",
    "mkdirp": "^0.5.1",
    "nodemon": "^1.11.0",
    "nyc": "^11.0.2",
    "pipe": "^1.1.0",
    "redrun": "^5.9.5",
    "request": "^2.79.0",
    "sinon": "^4.0.0",
    "sinon-called-with-diff": "^1.0.0",
    "tape": "^4.5.1",
    "webpack": "^3.8.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/coderaiser/node-restafary.git"
  },
  "keywords": [
    "rest",
    "crud",
    "files",
    "middleware"
  ],
  "author": "coderaiser <mnemonic.enemy@gmail.com> (http://coderaiser.github.io/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/coderaiser/node-restafary/issues"
  },
  "homepage": "https://github.com/coderaiser/node-restafary"
}
